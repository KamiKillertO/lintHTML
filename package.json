{
  "name": "linthtml",
  "version": "0.7.3",
  "description": "An unofficial html5 linter.",
  "repository": {
    "type": "git",
    "url": "http://github.com/KamiKillertO/linthtml"
  },
  "keywords": [
    "html",
    "lint",
    "hint",
    "linthtml"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "gulp tests",
    "coverage": "./node_modules/.bin/nyc --reporter=lcovonly --reporter=text npm test",
    "coverage:report": "npm run coverage; nyc report --reporter=lcov"
  },
  "license": "ISC",
  "devDependencies": {
    "bulkify": "1.4.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "gulp": "4.0.0",
    "gulp-coveralls": "0.1.4",
    "gulp-eslint": "5.0.0",
    "gulp-gh-pages": "0.5.4",
    "gulp-istanbul": "1.1.3",
    "gulp-jsdoc": "0.1.5",
    "gulp-mocha": "6.0.0",
    "mocha": "6.0.1",
    "nyc": "13.3.0"
  },
  "dependencies": {
    "bulk-require": "1.0.1",
    "htmlparser2": "3.10.1",
    "lodash": "4.17.11",
    "promise": "8.0.2"
  },
  "browserify": {
    "transform": [
      "bulkify"
    ]
  },
  "engines": {
    "node": ">=6"
  },
  "contributors": [
    "Benjamin JEGARD <kamikillerto@gmail.com> (https://github.com/KamiKillertO)"
  ]
}
